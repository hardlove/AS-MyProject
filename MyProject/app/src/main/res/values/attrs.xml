<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CornersButton" >

        <attr name="bg_pressed" format="reference|color"></attr>
        <attr name="bg_normal" format="reference|color"></attr>
        <attr name="text_pressed" format="reference|color"></attr>
        <attr name="text_normal" format="reference|color"></attr>
        <attr name="radius" format="reference|dimension"></attr>
    </declare-styleable>

    <declare-styleable name="CircleImageView">
        <attr name="civ_border_width" format="dimension" />
        <attr name="civ_border_color" format="color" />
        <attr name="civ_border_overlay" format="boolean" />
        <attr name="civ_fill_color" format="color" />
    </declare-styleable>
</resources>